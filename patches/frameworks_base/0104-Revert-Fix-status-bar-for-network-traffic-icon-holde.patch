From: Victor Bo <bvoid@yandex.ru>
Date: Wed, 1 Apr 2020 09:42:54 +0300
Subject: [PATCH] Revert "Fix status bar for network traffic icon holder"

This reverts commit 54def66726aa5a866319c2c683eb2fb0c60b8414.
---
 .../android/systemui/statusbar/phone/StatusBarIconList.java | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/packages/SystemUI/src/com/android/systemui/statusbar/phone/StatusBarIconList.java b/packages/SystemUI/src/com/android/systemui/statusbar/phone/StatusBarIconList.java
index e1f880c78c9..4d39930979b 100644
--- a/packages/SystemUI/src/com/android/systemui/statusbar/phone/StatusBarIconList.java
+++ b/packages/SystemUI/src/com/android/systemui/statusbar/phone/StatusBarIconList.java
@@ -36,7 +36,7 @@ public class StatusBarIconList {
         // Network traffic slot
         mSlots.add(0, new Slot(StatusBarNetworkTraffic.SLOT,
                     StatusBarIconHolder.fromNetworkTraffic()));
-        for (int i=1; i < N; i++) {
+        for (int i=0; i < N; i++) {
             mSlots.add(new Slot(slots[i], null));
         }
     }
@@ -49,8 +49,8 @@ public class StatusBarIconList {
                 return i;
             }
         }
-        // Auto insert new items at the beginning.
-        mSlots.add(0, new Slot(slot, null));
+        // Auto insert new items behind network traffic.
+        mSlots.add(1, new Slot(slot, null));
         return 0;
     }
 
